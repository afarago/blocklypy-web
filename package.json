{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "type": "commonjs",
  "files": [
    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "dev": "npm run start:console:dev",
    "console": "tsx  ./src/console.ts",
    "console:dev": "nodemon --exec tsx ./src/console.ts",
    "server": "tsx  ./src/server.ts",
    "server:dev": "nodemon --exec tsx ./src/server.ts",
    "curl1": "curl -X POST -F \"file=@testdata/test2.lms\" http://localhost:3000/upload",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "OLD_scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run lint",
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json ./app.ts",
    "dev": "ts-node-dev --transpile-only --project tsconfig.json src/index.ts",
    "start": "concurrently \"nodemon --watch src -e ts --exec ts-node src/index.ts\" \"browser-sync start --config bs-config.js\"",
    "start:dev": "nodemon --watch src -e ts --exec npm start dev"
  },
  "devDependencies": {
    "@types/express-fileupload": "^1.5.1",
    "@types/jquery": "^3.5.30",
    "@types/jszip": "^3.4.1",
    "@types/node": "^20.16.5",
    "@webpack-cli/generators": "^3.0.7",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "gts": "^5.3.1",
    "html-webpack-plugin": "^5.5.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.7.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "bootstrap-icons": "^1.11.3",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.1",
    "jszip": "^3.10.1"
  }
}
